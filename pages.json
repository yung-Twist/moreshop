{
	"easycom": {
		"autoscan": true,
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页"
			}
		}
	    ,{
            "path" : "pages/classify/classify",
            "style" : {
				"navigationBarTitleText": "分类"
			}
        }
        ,{
            "path" : "pages/store/store",
            "style" : {
				"navigationBarTitleText": "店铺"
			}
        },{
            "path" : "pages/shopcar/shopcar",
            "style" : {
				"navigationBarTitleText": "购物车"
			}
        }
        ,{
            "path" : "pages/mine/mine",
            "style" : {
				"navigationBarTitleText": "我的"
			}
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
	    "color": "#999999",
	    "selectedColor": "#1296db",
	    "borderStyle": "black",
	    "backgroundColor": "#ffffff",
	    "list": [
			{
				"pagePath": "pages/index/index",
				"iconPath": "static/images/tarbar/home.png",
				"selectedIconPath": "static/images/tarbar/homeA.png",
				"text": "首页"
			},{
				"pagePath": "pages/classify/classify",
				"iconPath": "static/images/tarbar/classify.png",
				"selectedIconPath": "static/images/tarbar/classifyA.png",
				"text": "分类"
			},{
				"pagePath": "pages/store/store",
				"iconPath": "static/images/tarbar/store.png",
				"selectedIconPath": "static/images/tarbar/storeA.png",
				"text": "店铺"
			},{
				"pagePath": "pages/shopcar/shopcar",
				"iconPath": "static/images/tarbar/shopcar.png",
				"selectedIconPath": "static/images/tarbar/shopcarA.png",
				"text": "购物车"
			},{
				"pagePath": "pages/mine/mine",
				"iconPath": "static/images/tarbar/mine.png",
				"selectedIconPath": "static/images/tarbar/mineA.png",
				"text": "我的"
			}
		]
	}
}
